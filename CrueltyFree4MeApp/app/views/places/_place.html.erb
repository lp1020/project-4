    <tr>
        <td class="placeInfo"><%= place.name %></td>
        <td class="placeInfo"><%= place.address %></td>
        <td class="placeInfo"><%= place.latitude %></td>
        <td class="placeInfo"><%= place.longitude %></td>
        <td class="placeButton"><%= link_to 'Show', place %></td>
        <td class="placeButton"><%= link_to 'Edit', edit_place_path(place) %></td>
        <td class="placeButton"><%= link_to 'Delete', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
